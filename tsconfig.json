{ 
  "compilerOptions": { 
    "target": "es5", 
    "lib": ["DOM", "DOM.Iterable", "ESNext", "ES2015"], 
    "strict": false, 
    "module": "ES6", 
    "moduleResolution": "node", 
    "sourceMap": true, 
    "noEmit": true, 
    "esModuleInterop": true, 
    "allowJs": true,    
    "forceConsistentCasingInFileNames": false, 
    "allowSyntheticDefaultImports": true, 
    "skipLibCheck": true, 
    "noEmitOnError": true, 
    "noImplicitAny": false, 
    "types": ["@types/node","@types/lodash"],     
    "outDir": "./tests-out", 
    "rootDir": ".", 
    "baseUrl": ".", 
  }, 
  "include": ["*.ts"],  
  "exclude": ["*.js"], 
  "paths": { 
    "@pages/*": ["src/pages-objects/*"],
    "@services/*": ["src/services/*"],    
    "@constants/*": ["src/constants/**/*"],
    "@utils/*": ["src/utils/**/*"],
    "@fixtures/*": ["src/fixtures/*"],
  }, 
  "files": [ 
    "global.d.ts",
    "src/constants/types/APITypes.d.ts",
    "src/constants/types/NavigationTypes.d.ts",
    "src/constants/types/ReportTypes.d.ts", 
  ]  
}

